{
    "name": "teodoriu/laravel-whatsapp",
    "description": "A Whatsapp Business Cloud API wrapper for Laravel.",
    "keywords": [
        "laravel",
        "whatsapp",
        "facebook",
        "sdk",
        "api"
    ],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Teodoriu\\Whatsapp\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Teodor B.",
            "email": "teodoriu@pm.me"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^9.0 || ^10.0 || ^11.0",
        "illuminate/support": "^9.0 || ^10.0 || ^11.0",
        "illuminate/http": "^9.0 || ^10.0 || ^11.0",
        "illuminate/routing": "^9.0 || ^10.0 || ^11.0",
        "illuminate/events": "^9.0 || ^10.0 || ^11.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Teodoriu\\Whatsapp\\WhatsappServiceProvider"
            ],
            "aliases": {
                "Whatsapp": "Teodoriu\\Whatsapp\\Facade\\Whatsapp"
            }
        }
    }
}